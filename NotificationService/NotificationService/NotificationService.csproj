      <UserSecretsId>14d62ed3-a66c-4764-a5ea-918954b4395d</UserSecretsId>
      <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
      <DockerfileContext>.</DockerfileContext>
      <RootNamespace>Photography.Management.Suite.NotificationService</RootNamespace>
      <AssemblyName>Photography.Management.Suite.NotificationService</AssemblyName>
      <SignAssembly>False</SignAssembly>
      <Authors>Provident Software</Authors>
      <Product>Photography Management Suite</Product>
